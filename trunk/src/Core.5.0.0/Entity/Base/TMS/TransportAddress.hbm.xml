<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
 	<class name="com.Sconit.Entity.TMS.TransportAddress, com.Sconit.Entity" table="TMS_Address" lazy="false">
		<id name="Code" column="Code" type="String" unsaved-value="null" >
      		<generator class="assigned" />
        </id>
		<property name="Country" column="Country" type="String" length="50" />	
		<property name="Province" column="Province" type="String" length="50" />	
		<property name="City" column="City" type="String" length="50" />	
		<property name="District" column="District" type="String" length="50" />	
		<property name="Street" column="Street" type="String" length="50" />
		<property name="Address" column="Address" type="String" length="255" />
		<property name="PostCode" column="Postcode" type="String" length="50" />
		<property name="Contacts" column="Contacts" type="String" length="50" />
		<property name="Phone" column="Phone" type="String" length="50" />
		<property name="MobilePhone" column="MobilePhone" type="String" length="50" />
		<property name="Email" column="Email" type="String" length="50" />
		<property name="Remark" column="Remark" type="String" length="255" />
		<property name="CreateDate" column="CreateDate" type="DateTime" update="false"/>
		<property name="CreateUserId" column="CreateUser" type="Int32" update="false"/>
		<property name="CreateUserName" column="CreateUserNm" type="String" length="50" update="false"/>
		<property name="LastModifyDate" column="LastModifyDate" type="DateTime"  />
		<property name="LastModifyUserId" column="LastModifyUser" type="Int32" />
		<property name="LastModifyUserName" column="LastModifyUserNm" type="String" length="50" />
  	</class>
</hibernate-mapping>
