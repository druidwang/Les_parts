@using com.Sconit.Entity.SYS
@model com.Sconit.Entity.TMS.TransportAddress
@{
    ViewBag.Title = Resources.TMS.TransportAddress.TransportAddress_Edit_Title;
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_TransportAddress_View" });
}
@using (Html.BeginForm())
{
    <fieldset>
        <legend>@Resources.TMS.TransportAddress.TransportAddress_Edit_Title</legend>
        <div class="editor">
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Code)
                @Html.HiddenFor(model => model.Code)
            </div>
            <div class="editor-field-2">
                @Html.DisplayFor(model => model.Code)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Country)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Country)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Province)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Province)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.City)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.City)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.District)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.District)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Street)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Street)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Address)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Address)
            </div>
        </div>
        <div class="editor-footer">
            <button type="submit" id="btnSave">
                @Resources.Global.Button_Save
            </button>
            <button type="button" id="btnDelete" onclick="location.href='@Url.Content("~/TransportAddress/Delete/" + @Model.Code)'">
                @Resources.Global.Button_Delete
            </button>
            <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/TransportAddress/List")'">
                @Resources.Global.Button_Back
            </button>
        </div>
    </fieldset>
}
